
class Animal 
{
	String type;
	String name;
	int age;
	int weight;
	void eat()
	{
		System.out.println("animal eat");
				
	}
	void breath(){
		System.out.println("animal breath");
	}
	void sleep(){
		System.out.println("animal sleep");
	}

}
class Tiger extends Animal{
	String tigerType;
	String from;
	void tigerRun(){
		System.out.println("the tiger run");
	}
}

